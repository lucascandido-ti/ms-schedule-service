{
  "name": "ms-schedule-service",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "ddd": "pnpm --filter @schedule/ddd",
    "core": "pnpm --filter @schedule/core",
    "api": "pnpm --filter @schedule/api",
    "build": "turbo run build",
    "clean": "turbo run clean --parallel",
    "dev": "turbo run dev --filter=@schedule/* --parallel",
    "format": "turbo run format --parallel",
    "lint": "turbo run lint --parallel",
    "test": "turbo run test --parallel",
    "typecheck": "turbo run typecheck --parallel"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "turbo": "^2.0.9",
    "typescript": "^5.1.3"
  }
}
